{
  "name": "aws-parameter-store-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:prod": "vue-cli-service build --mode production",
    "push:pages": "git subtree push --prefix dist origin gh-pages",
    "deploy:pages": "npm run build:prod && git add . && git commit -m \"Update dist\" && npm run push:pages && git reset HEAD~1 --hard",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "aws-sdk": "^2.481.0",
    "buefy": "^0.7.7",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
